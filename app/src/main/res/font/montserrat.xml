<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <!--regular-->
    <font
        android:font="@font/montserrat_regular"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/montserrat_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        tools:ignore="ResourceCycle" />
    <font
        android:font="@font/montserrat_bold"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/montserrat_bold"
        app:fontStyle="normal"
        app:fontWeight="400"
        tools:ignore="ResourceCycle" />
    <font
        android:font="@font/montserrat_italic"
        android:fontStyle="italic"
        android:fontWeight="400"

        app:font="@font/montserrat_italic"
        app:fontStyle="italic"
        app:fontWeight="400"
        tools:ignore="ResourceCycle" />
    <font
        android:font="@font/montserrat_medium"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/montserrat_medium"
        app:fontStyle="normal"
        app:fontWeight="400"
        tools:ignore="ResourceCycle" />

    <font
        android:font="@font/montserrat_semibold"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/montserrat_semibold"
        app:fontStyle="normal"
        app:fontWeight="400"
        tools:ignore="ResourceCycle" />


</font-family>